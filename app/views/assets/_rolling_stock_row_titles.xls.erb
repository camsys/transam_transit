<% is_vehicle = @view.starts_with?('vehicle') %>
<% is_support_vehicle = @view.starts_with?('support_vehicle') %>
<% is_rail_car = @view.starts_with?('rail_car') %>
<% is_locomotive = @view.starts_with?('locomotive') %>
<% is_passenger_vehicle = is_vehicle || is_rail_car %>


<Cell><Data ss:Type="String">Manufacturer</Data></Cell>
<Cell><Data ss:Type="String">Model</Data></Cell>
<Cell><Data ss:Type="String">Manufacture Year</Data></Cell>
<Cell><Data ss:Type="String">Vehicle Length</Data></Cell>

<Cell><Data ss:Type="String">Title Owner</Data></Cell>
<Cell><Data ss:Type="String">Purchased New</Data></Cell>
<Cell><Data ss:Type="String">Purchase Cost</Data></Cell>
<Cell><Data ss:Type="String">Purchase Date</Data></Cell>

<Cell><Data ss:Type="String">Est. Useful Life</Data></Cell>
<Cell><Data ss:Type="String">VIN</Data></Cell>
<Cell><Data ss:Type="String">Rebuild Year</Data></Cell>

<Cell><Data ss:Type="String">Description</Data></Cell>

<Cell><Data ss:Type="String">FTA Ownership Type</Data></Cell>

<Cell><Data ss:Type="String">ADA Lift</Data></Cell>
<Cell><Data ss:Type="String">ADA Ramp</Data></Cell>
<Cell><Data ss:Type="String">Emergency Fleet</Data></Cell>
<Cell><Data ss:Type="String">FTA Mode Types</Data></Cell>
<Cell><Data ss:Type="String">FTA Bus Mode Type</Data></Cell>
<Cell><Data ss:Type="String">FTA Service Types</Data></Cell>

<% if is_passenger_vehicle %>
  <Cell><Data ss:Type="String">Seating Capacity</Data></Cell>
  <Cell><Data ss:Type="String">Standing Capacity</Data></Cell>
  <Cell><Data ss:Type="String">Wheelchair Capacity</Data></Cell>
  <Cell><Data ss:Type="String">Vehicle Features</Data></Cell>

  <% if is_vehicle %>
    <Cell><Data ss:Type="String">Fuel Type</Data></Cell>
    <Cell><Data ss:Type="String">Expected Useful Miles</Data></Cell>
    <Cell><Data ss:Type="String">License Plate</Data></Cell>
  <% end %>

<% end %>

<% if is_locomotive %>
  <Cell><Data ss:Type="String">Fuel Type</Data></Cell>
<% end %>

<% if is_support_vehicle %>
  <Cell><Data ss:Type="String">License Plate</Data></Cell>
  <Cell><Data ss:Type="String">Seating Capacity</Data></Cell>
  <Cell><Data ss:Type="String">Fuel Type</Data></Cell>
<% end %>
