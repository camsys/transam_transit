= format_field("Manufacturer", @asset.manufacturer.name )
= format_field("Other Manufacturer", @asset.other_manufacturer ) unless @asset.other_manufacturer.blank?
= format_field("Model", @asset.manufacturer_model )
= format_field("Manufacture Year", @asset.manufacture_year )
= format_field("Vehicle Identification Number", @asset.serial_number )
= format_field("License Plate", @asset.license_plate )
= format_field("Fuel Type", @asset.fuel_type )
= format_field("Dual Fuel Type", @asset.dual_fuel_type ) unless @asset.dual_fuel_type.nil?
= format_field("Other Fuel Type", @asset.other_fuel_type) unless @asset.other_fuel_type.blank?
= format_field("Gross Vehicle Weight (lbs)", format_as_integer(@asset.gross_vehicle_weight) )
= format_field("Seating Capacity", format_as_integer(@asset.seating_capacity) )

- view_component = 'app_support_vehicle_configuration'
- if lookup_context.template_exists?(view_component, 'assets', true)
  = render :partial => "assets/#{view_component}"