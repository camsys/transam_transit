.tab-pane.fade#identify
  .tab-content
    = render "transit_assets/#{@asset.class.name.underscore}/identification_form"
.tab-pane.fade#chars
  .tab-content
    = render "transit_assets/#{@asset.class.name.underscore}/characteristics_form"
- geometry_view_component = "#{@asset.class.name.underscore}_geometry_form"
- if lookup_context.template_exists?(geometry_view_component, 'transit_assets', true)
  .tab-pane.fade#geometry
    .tab-content
      = render "transit_assets/#{geometry_view_component}"
- system_view_component = "#{@asset.class.name.underscore}_system_form"
- if lookup_context.template_exists?(system_view_component, 'transit_assets', true)
  .tab-pane.fade#sys
    .tab-content
      = render "transit_assets/#{system_view_component}"
.tab-pane.fade#fund
  .tab-content
    = render "transit_assets/#{@asset.class.name.underscore}/funding_form"
.tab-pane.fade#purchase
  .tab-content
    = render "transit_assets/#{@asset.class.name.underscore}/purchase_form"
.tab-pane.fade#ops
  .tab-content
    = render "transit_assets/#{@asset.class.name.underscore}/operations_form"
- reg_view_component = "registration_form"
- if lookup_context.template_exists?(reg_view_component, "transit_assets/#{@asset.class.name.underscore}", true)
  .tab-pane.fade#reg
    .tab-content
      = render "transit_assets/#{@asset.class.name.underscore}/#{reg_view_component}"