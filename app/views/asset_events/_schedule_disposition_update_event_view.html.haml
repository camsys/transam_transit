= dialog_tag('Scheduled Disposition', {:icon => "#{ScheduleDispositionUpdateEvent.asset_event_type.display_icon_name}", :class => "header_part"}) do
  = format_field("Anticipated Disposition Date", @asset_event.disposition_date)
  = format_field("Reported On", format_as_date(@asset_event.event_date))
  = format_field("Comments", @asset_event.comments.html_safe) unless @asset_event.comments.blank?
