= dialog_tag('Service Status Update', {:icon => "#{ServiceStatusUpdateEvent.asset_event_type.display_icon_name}", :class => "header_part"}) do
  = format_field("Service Status Type", @asset_event.service_status_type.name)
  = format_field("Reported On", format_as_date(@asset_event.event_date))
  = format_field("Comments", @asset_event.comments.html_safe) unless @asset_event.comments.blank?
