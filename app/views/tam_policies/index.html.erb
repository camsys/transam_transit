<p id="notice"><%= notice %></p>

<h1>Listing Tam Policies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tam_policies.each do |tam_policy| %>
      <tr>
        <td><%= link_to 'Show', tam_policy %></td>
        <td><%= link_to 'Edit', edit_tam_policy_path(tam_policy) %></td>
        <td><%= link_to 'Destroy', tam_policy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tam policy', new_tam_policy_path %>
