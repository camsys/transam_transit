-if @fta_asset_class.code.in? ["bus", "rail_car", "ferry", "other_passenger_vehicle"]
  %h2 Revenue Vehicles
  = render :partial => 'transit_assets/revenue_vehicle_index_table'
  = render :partial => 'transit_assets/rail_car_index_table'
  = render :partial => 'transit_assets/ferry_index_table'
  = render :partial => 'transit_assets/other_passenger_vehicle_index_table'
-elsif @fta_asset_class.name.in? ["track", "guideway", "power_signal"]
  %h2 Infrastructure
  = render :partial => 'transit_assets/track_index_table'
  = render :partial => 'transit_assets/guideway_index_table'
  = render :partial => 'transit_assets/power_signal_index_table'
-else 
  =@fta_asset_class.name